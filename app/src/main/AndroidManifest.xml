<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.traveldiaries">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.TravelDiaries">
        <activity android:name=".VisitedCitiesList"/>
        <activity android:name=".CityInformation" />
        <activity android:name=".VisitedCityDescription" />
        <activity android:name=".AboutUs" />
        <activity android:name=".MyTrips" />
        <activity android:name=".Profile" />
        <activity android:name=".UserGuide" />
        <activity android:name=".SignupUser2" />
        <activity android:name=".SignupUser" />
        <activity android:name=".LoginUser" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>